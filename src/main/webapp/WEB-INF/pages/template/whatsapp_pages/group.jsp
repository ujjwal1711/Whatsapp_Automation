         	  alert("empty fields");
         	}
         	else
             {
                var groupname=$("#id1").val();
                var tag=$("#id2").val();

                var data={"groupname":groupname,"tag":tag};
                $.ajax({
                              type: "POST",
                              url:  "/whatsapp/data",
                              data: JSON.stringify(data),
                              contentType: 'application/json',
                              success: function(response) {
                                      if(!!onSuccess&&typeof onSuccess ==="function"){
                                                         	window.location.href="/whatsapp/main";
                                                       }
                              },
                              error:function(data){
                               if(!!onFailure&&typeof onFailure ==="function"){
